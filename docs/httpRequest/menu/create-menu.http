# Create Menu
# POST /api/menus
# Creates a new menu item

POST http://localhost:3001/api/menus
Content-Type: application/json

{
  "name": "Nasi Goreng Spesial",
  "description": "Nasi goreng dengan telur, ayam, dan sayuran segar",
  "isActive": true
}

